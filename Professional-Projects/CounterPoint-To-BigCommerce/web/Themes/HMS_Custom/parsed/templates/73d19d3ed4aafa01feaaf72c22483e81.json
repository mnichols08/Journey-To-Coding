{"components/faceted-search/facets/rating":"<ul class=\"faceted-search-filter-list\">\n  {{#each items}}\n    {{!--\n      TODO: Remove this check when platform bug fixed.\n      Rating facets sometimes provides an item without a URL.\n    --}}\n    {{#if url}}\n      <li class=\"faceted-search-item\">\n        <a\n          href=\"{{url}}\"\n          rel=\"nofollow\"\n          title=\"{{lang 'search.faceted.rating.and_up' rating=param_value}}\"\n          data-faceted-search-facet\n        >\n          <input\n            type=\"checkbox\"\n            class=\"facet-checkbox\"\n            {{#if selected}}checked{{/if}}\n          >\n\n          <span class=\"faceted-search-item-label facet-rating\">\n            <div class=\"rating-stars stars-{{size}}\" aria-hidden=\"true\">\n              {{#for 1 5}}\n                {{#if ../param_value '>=' $index}}\n                  <span class=\"star pxu-star checked\"></span>\n                {{else}}\n                  <span class=\"star pxu-star\"></span>\n                {{/if}}\n              {{/for}}\n            </div>\n\n            {{#if ../show_product_counts}}\n              <span class=\"faceted-search-item-count\">({{ count }})</span>\n            {{/if}}\n          </span>\n        </a>\n      </li>\n    {{/if}}\n  {{/each}}\n</ul>\n"}