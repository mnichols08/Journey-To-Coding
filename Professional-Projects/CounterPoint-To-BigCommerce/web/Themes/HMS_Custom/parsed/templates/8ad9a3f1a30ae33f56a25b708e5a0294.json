{"components/common/footer":"{{#if settings.show_newsletter_box}}\n  {{> components/sections/subscription-form}}\n{{/if}}\n\n{{> components/common/banners banners=banners.bottom class='banners-bottom'}}\n\n<footer class=\"main-footer\">\n  <div class=\"container\">\n    <div class=\"columns footer-columns\">\n      <div class=\"logo-column\">\n        <div class=\"footer-branding\">\n          {{#if settings.store_logo.image.data}}\n            <a class=\"logo\" href=\"{{urls.home}}\">\n              <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo'}}\" alt=\"{{settings.store_logo.image.alt}}\">\n            </a>\n          {{else}}\n            <h1 class=\"text\"><a href=\"{{urls.home}}\">{{settings.store_logo.title}}</a></h1>\n          {{/if}}\n        </div>\n        {{#if theme_settings.geotrust-ssl-common-name}}\n          {{> core/geotrust-ssl-seal}}\n        {{/if}}\n      </div>\n\n      <div class=\"footer-columns-container\">\n\n        {{#if theme_settings.footer_show_contactinfo}}\n          <div class=\"footer-column\">\n            <h4 class=\"footer-column-heading\">{{lang 'footer.contact'}}</h4>\n            <div class=\"shop-contact\">\n              {{#if settings.address}}\n                <div class=\"shop-contact-item\">\n                  <p class=\"shop-contact-item-details\">{{nl2br settings.address}}</p>\n                </div>\n              {{/if}}\n\n              {{#if settings.phone_number.length}}\n                <div class=\"shop-contact-item\">\n                  <p class=\"shop-contact-item-details\">{{settings.phone_number}}</p>\n                </div>\n              {{/if}}\n            </div>\n          </div>\n        {{/if}}\n\n        {{#if theme_settings.footer_show_pages}}\n          <div class=\"footer-column\">\n            <h4 class=\"footer-column-heading\">{{lang 'footer.pages'}}</h4>\n            <nav class=\"footer-nav\">\n              <ul>\n                {{#each pages}}\n                  <li>\n                    <a href=\"{{url}}\">\n                      {{name}}\n                    </a>\n                  </li>\n                {{/each}}\n\n                {{#if urls.sitemap}}\n                  <li>\n                    <a href=\"{{urls.sitemap}}\">{{lang 'common.sitemap'}}</a>\n                  </li>\n                {{/if}}\n              </ul>\n            </nav>\n          </div>\n        {{/if}}\n\n        {{#if theme_settings.footer_show_categories}}\n          <div class=\"footer-column\">\n            <h4 class=\"footer-column-heading\">{{lang 'category.label'}}</h4>\n            <nav class=\"footer-nav\">\n              <ul>\n                {{#each categories}}\n                  <li>\n                    <a href=\"{{url}}\">\n                      {{name}}\n                    </a>\n                  </li>\n                {{/each}}\n              </ul>\n            </nav>\n          </div>\n        {{/if}}\n\n        {{#if theme_settings.footer_show_brands}}\n          <div class=\"footer-column\">\n            <h4 class=\"footer-column-heading\">{{lang 'brand.label'}}</h4>\n            <nav class=\"footer-nav\">\n              <ul>\n                {{#each shop_by_brand}}\n                  {{#compare @index 5 operator=\"<\"}}\n                    <li>\n                      <a href=\"{{url}}\">{{name}}</a>\n                    </li>\n                  {{/compare}}\n                {{/each}}\n                {{#compare shop_by_brand.length 5 operator=\">\"}}\n                  <li>\n                    <a href=\"{{urls.brands}}\" class=\"footer-nav-view-all\">{{lang 'brand.view_brands'}} &rsaquo;</a>\n                  </li>\n                {{/compare}}\n              </ul>\n            </nav>\n          </div>\n        {{/if}}\n\n      </div>\n    </div>\n\n    <div class=\"footer-lower\">\n\n      {{#if social_media.length}}\n        {{> components/common/social-media-links links=social_media}}\n      {{/if}}\n\n      <div class=\"footer-lower-left\">\n\n        <ul class=\"footer-lower-nav\">\n          {{#if settings.gift_certificates_enabled}}\n            <li class=\"footer-lower-nav-item\">\n              <a href=\"{{urls.gift_certificate.purchase}}\">\n                {{lang 'footer.gift_certificates'}}\n              </a>\n            </li>\n          {{/if}}\n          {{#if settings.account_creation_enabled}}\n            {{#if customer}}\n              <li class=\"footer-lower-nav-item\">\n                <a href=\"{{urls.account.index}}\" title=\"{{lang 'header.logged_in_as' name=customer.name}}\">\n                  <span class=\"my-account\">{{lang 'header.my_account'}}</span>\n                </a>\n              </li>\n              <li class=\"footer-lower-nav-item\">\n                <a href=\"{{urls.auth.logout}}\">{{lang 'header.log_out' }}</a>\n              </li>\n            {{else}}\n              <li class=\"footer-lower-nav-item\">\n                <a href=\"{{urls.auth.create_account}}\">{{lang 'footer.create_account'}}</a>\n              </li>\n              <li class=\"footer-lower-nav-item\">\n                <a href=\"{{urls.auth.login}}\">{{lang 'footer.sign_in'}}</a>\n              </li>\n            {{/if}}\n          {{/if}}\n        </ul>\n        <div class=\"footer-site-info\n          {{#if theme_settings.hide_copyright}}{{#if theme_settings.hide_theme_credits}}hide-footer-site-info{{/if}}{{/if}}\">\n            {{#unless theme_settings.hide_copyright}}\n              <span class=\"footer-copyright\">\n                &copy; <script>document.write(new Date().getFullYear())</script> <a href=\"{{settings.base_url}}\">{{settings.store_name}}</a>\n              </span>\n            {{/unless}}\n            {{#unless theme_settings.hide_theme_credits}}\n              <div class=\"footer-credits\">\n                Powered by <a href=\"http://bigcommerce.com\" target=\"_blank\" rel=\"nofollow\">BigCommerce</a>. Venture theme by <a href=\"http://pixelunion.net\" target=\"_blank\">Pixel Union.</a>\n              </div>\n            {{/unless}}\n        </div>\n        {{> components/common/payment-icons}}\n      </div>\n    </div>\n  </div>\n</footer>\n","components/sections/subscription-form":"<aside class=\"section-subscription-form\">\n  <div class=\"container\">\n    <div class=\"section-header\">\n      <h3 class=\"section-title\">{{lang 'newsletter.subscribe'}}</h3>\n    </div>\n    <form\n      class=\"form subscription-form\"\n      action=\"{{urls.subscribe.action}}\"\n      method=\"post\"\n      data-validates\n    >\n      <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n      <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n      <input type=\"hidden\" name=\"check\" value=\"1\">\n      <div class=\"form-field form-inline\">\n        <label class=\"sr-only\" for=\"nl_email\">\n          {{lang 'newsletter.email_placeholder'}}\n        </label>\n        <input\n          class=\"form-input\"\n          id=\"nl_email\"\n          name=\"nl_email\"\n          type=\"email\"\n          value=\"{{customer.name}}\"\n          placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n        >\n        <input\n          class=\"button button-secondary\"\n          type=\"submit\"\n          value=\"{{lang 'newsletter.subscribe_submit'}}\"\n        >\n      </div>\n    </form>\n    {{#if settings.show_newsletter_summary}}\n      <div class=\"subscription-summary\">{{settings.newsletter_summary}}</div>\n    {{/if}}\n  </div>\n</aside>\n","components/common/banners":"{{#if banners}}\n  <div class=\"banners {{class}}\">\n    <div class=\"banner rte\">\n      {{{limit banners 1}}}\n    </div>\n  </div>\n{{/if}}\n","components/common/social-media-links":"<ul class=\"footer-social-media\">\n    {{#each links}}\n      <li class=\"social-media-link\">\n        <a href=\"{{url}}\" class=\"{{display_name}}\" target=\"_blank\">\n          <svg class=\"icon-social\">\n            <use xlink:href=\"#icon-{{#if display_name '===' 'google+'}}google-plus{{else}}{{display_name}}{{/if}}\"></use>\n          </svg>\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n","core/geotrust-ssl-seal":"<table\n  class=\"geotrust-ssl-seal-table\"\n  width=\"135\"\n  border=\"0\"\n  cellpadding=\"2\"\n  cellspacing=\"0\"\n  title=\"{{lang 'core.geotrust.click_to_verify'}}\"\n>\n  <tr>\n    <td width=\"135\" align=\"center\" valign=\"top\">\n      <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust-ssl-common-name}}&amp;size={{theme_settings.geotrust-ssl-seal-size}}&amp;lang={{locale_name}}\"></script><br />\n      <a\n        href=\"http://www.geotrust.com/ssl/\"\n        target=\"_blank\"\n        style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"\n      ></a>\n    </td>\n  </tr>\n</table>\n","components/common/payment-icons":"<ul class=\"payment-options\">\n  {{#if theme_settings.payment_visa}}\n    <li class=\"payment-option-item\">\n      <svg class=\"footer-payment-icon\" viewBox=\"0 0 1582 1024\">\n        <title>Visa</title>\n        <path d=\"M449.88 329.060l135.69-0.001-201.66 495.090-135.54 0.080-104.28-394.52c74.18 30.47 140.14 96.7 167.010 168.79l13.44 68.731zM557.311 824.669l80.109-496.020h128.040l-80.1 496.020h-128.050zM1023.73 527.82c73.91 35.38 108 78.2 107.58 134.75-1.040 102.93-88.49 169.43-223.030 169.43-57.481-0.63-112.82-12.62-142.7-26.44l17.92-111.020 16.481 7.88c42.12 18.53 69.34 26.040 120.62 26.040 36.83 0 76.32-15.22 76.66-48.45 0.21-21.731-16.54-37.27-66.45-61.57-48.56-23.68-112.949-63.449-112.169-134.739 0.76-96.39 89.899-163.71 216.489-163.71 49.59 0 89.439 10.86 114.76 20.83l-17.33 107.52-11.49-5.68c-23.68-10.060-53.979-19.739-95.85-19.039-50.17 0-73.36 22.060-73.37 42.659-0.33 23.3 27.13 38.53 71.88 61.54zM1478.36 329.12l103.651 495.679h-118.86s-11.78-57.030-15.601-74.3c-18.67 0-149.3