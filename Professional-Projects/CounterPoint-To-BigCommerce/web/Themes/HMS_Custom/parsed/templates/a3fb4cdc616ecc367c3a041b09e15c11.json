{"components/blog/blog-index":"<div class=\"blog-index\">\n  <section class=\"blog-items-list\" itemscope itemtype=\"http://schema.org/Blog\">\n    {{#each blog.posts}}\n      {{> components/blog/blog-item this show_thumb=../theme_settings.show_blog_thumbnails}}\n    {{/each}}\n  </section>\n\n  {{#if blog.posts.length}}\n    {{> components/common/pagination pagination=pagination.blog}}\n  {{/if}}\n\n  <div class=\"collection-progress-overlay\">\n    <div class=\"inner\">\n      {{> components/common/spinner spinner_class='spinner-dark'}}\n      {{lang 'common.loading'}}\n    </div>\n  </div>\n</div>","components/common/pagination":"<div class=\"pagination-container\">\n\n  {{#if pagination.links.length}}\n    <div class=\"pagination\">\n      {{#if pagination.previous}}\n        <a class=\"pagination-item previous\" href=\"{{pagination.previous}}\" data-pagination>\n          <span class=\"icon pxu-chevron-rounded-left\" aria-hidden=\"true\"></span>\n          {{lang 'common.pagination.previous'}}\n          <span class=\"page\"> {{lang 'common.pagination.page'}}</span>\n        </a>\n      {{/if}}\n\n      {{#each pagination.links}}\n        {{#if number '===' ../pagination.current}}\n          <span class=\"pagination-item pagination-page pagination-current\">{{number}}</span>\n        {{else}}\n          <a class=\"pagination-item pagination-page\" href=\"{{url}}\" data-pagination>{{number}}</a>\n        {{/if}}\n      {{/each}}\n\n      {{#if pagination.next}}\n        <a class=\"pagination-item next\" href=\"{{pagination.next}}\" data-pagination>\n          {{lang 'common.pagination.next'}}\n          <span class=\"page\"> {{lang 'common.pagination.page'}}</span>\n          <span class=\"icon pxu-chevron-rounded-right\" aria-hidden=\"true\"></span>\n        </a>\n      {{/if}}\n    </div>\n  {{/if}}\n\n  <a class=\"to-top\" data-to-top href=\"#top\">{{lang 'common.back_to_top'}}</a>\n</div>\n","components/blog/blog-item":"<article class=\"blog-item\"  itemscope itemtype=\"http://schema.org/BlogPosting\">\n  <meta itemprop=\"url\" content=\"{{url}}\">\n\n  {{#if show_thumb}}\n    <figure class=\"blog-image-container {{#unless thumbnail}}blog-image-container-empty{{/unless}}\">\n      {{#if thumbnail}}\n        <a href=\"{{url}}\" title=\"{{title}}\">\n          <img src=\"{{getImage thumbnail 'blog-thumbnail'}}\" alt=\"{{title}}\" itemprop=\"image\">\n        </a>\n      {{/if}}\n    </figure>\n  {{/if}}\n\n  <time class=\"timestamp\" itemprop=\"datePublished\">{{date_published}}</time>\n  <a href=\"{{url}}\" title=\"{{title}}\">\n    <h4 class=\"title\" itemprop=\"headline\">{{title}}</h4>\n  </a>\n  <div class=\"excerpt\" itemprop=\"articleBody\">\n    {{#if summary}}\n      {{{summary}}}{{#if summary.length \">=\" 150}}&hellip;{{/if}}\n    {{else}}\n      {{{body}}}\n    {{/if}}\n  </div>\n  <a href=\"{{url}}\" title=\"{{title}}\" class=\"continue-reading\">{{lang 'blog.continue_reading'}} &rsaquo;</a>\n</article>\n","components/common/spinner":"<div class=\"pxu-fading-circle {{spinner_class}}\">\n  <div class=\"pxu-circle1 pxu-circle\"></div>\n  <div class=\"pxu-circle2 pxu-circle\"></div>\n  <div class=\"pxu-circle3 pxu-circle\"></div>\n  <div class=\"pxu-circle4 pxu-circle\"></div>\n  <div class=\"pxu-circle5 pxu-circle\"></div>\n  <div class=\"pxu-circle6 pxu-circle\"></div>\n  <div class=\"pxu-circle7 pxu-circle\"></div>\n  <div class=\"pxu-circle8 pxu-circle\"></div>\n  <div class=\"pxu-circle9 pxu-circle\"></div>\n  <div class=\"pxu-circle10 pxu-circle\"></div>\n  <div class=\"pxu-circle11 pxu-circle\"></div>\n  <div class=\"pxu-circle12 pxu-circle\"></div>\n</div>\n"}