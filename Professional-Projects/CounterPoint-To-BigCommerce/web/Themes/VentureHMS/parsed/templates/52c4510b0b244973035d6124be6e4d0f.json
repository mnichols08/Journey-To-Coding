{"components/sections/subscription-form":"<aside class=\"section-subscription-form\">\n  <div class=\"container\">\n    <div class=\"section-header\">\n      <h3 class=\"section-title\">{{lang 'newsletter.subscribe'}}</h3>\n    </div>\n    <form\n      class=\"form subscription-form\"\n      action=\"{{urls.subscribe.action}}\"\n      method=\"post\"\n      data-validates\n    >\n      <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n      <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n      <input type=\"hidden\" name=\"check\" value=\"1\">\n      <div class=\"form-field form-inline\">\n        <label class=\"sr-only\" for=\"nl_email\">\n          {{lang 'newsletter.email_placeholder'}}\n        </label>\n        <input\n          class=\"form-input\"\n          id=\"nl_email\"\n          name=\"nl_email\"\n          type=\"email\"\n          value=\"{{customer.name}}\"\n          placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n        >\n        <input\n          class=\"button button-secondary\"\n          type=\"submit\"\n          value=\"{{lang 'newsletter.subscribe_submit'}}\"\n        >\n      </div>\n    </form>\n    {{#if settings.show_newsletter_summary}}\n      <div class=\"subscription-summary\">{{settings.newsletter_summary}}</div>\n    {{/if}}\n  </div>\n</aside>\n"}