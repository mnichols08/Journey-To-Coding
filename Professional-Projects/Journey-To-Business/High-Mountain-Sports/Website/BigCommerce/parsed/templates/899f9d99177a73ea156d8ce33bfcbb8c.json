{"components/brand/show-more":"{{#if brand.facets}}\n  {{> components/faceted-search/show-more-facets brand}}\n{{/if}}\n","components/faceted-search/show-more-facets":"<ul class=\"nav-list faceted-search-option-columns\" data-facet-filter-wrapper>\n  {{#each facets}}\n    {{#if facet '===' ../all_facets}}\n      {{#each items}}\n        <li class=\"faceted-search-item {{#compare @index 5 operator='>='}}hidden{{/compare}}\">\n          <a href=\"{{url}}\" title=\"{{title}}\" rel=\"nofollow\" data-faceted-search-facet>\n            <input type=\"checkbox\" class=\"facet-checkbox\" {{#if selected}}checked{{/if}}>\n            <span class=\"faceted-search-item-label\">\n              {{title}}\n              {{#if ../show_product_counts}}<span class=\"faceted-search-item-count\">({{ count }})</span>{{/if}}\n            </span>\n          </a>\n        </li>\n      {{/each}}\n    {{/if}}\n  {{/each}}\n</ul>\n"}
