{"components/faceted-search/facets/range":"<form class=\"faceted-search-filter-list\" method=\"get\" data-faceted-search-range>\n  {{#each current_selected_items}}\n    <input type=\"hidden\" name=\"{{param_name}}[]\" value=\"{{param_value}}\"/>\n  {{/each}}\n\n  <input type=\"hidden\" name=\"search_query\" value=\"{{search_query}}\">\n\n  {{#if sort}}\n    <input type=\"hidden\" name=\"sort\" value=\"{{sort}}\">\n  {{/if}}\n\n  <div class=\"facet-range-input-group\">\n    <div class=\"facet-range-inputs\">\n      <div class=\"form-field faceted-search-range-min\">\n        <input\n          class=\"form-input\"\n          name=\"min_price\"\n          min=\"0\"\n          type=\"number\"\n          value=\"{{min_price}}\"\n          placeholder=\"{{ lang 'category.sidebar.min_price'}}\"\n          required\n        />\n      </div>\n\n      <div class=\"form-field faceted-search-range-max\">\n        <input\n          class=\"form-input\"\n          name=\"max_price\"\n          min=\"0\"\n          type=\"number\"\n          value=\"{{max_price}}\"\n          placeholder=\"{{lang 'category.sidebar.max_price'}}\"\n          required\n        />\n      </div>\n    </div>\n\n    <input\n      class=\"button button-primary button-small facet-submit\"\n      type=\"submit\"\n      value=\"{{lang 'category.sidebar.update'}}\"\n    >\n  </div>\n</form>\n"}
