{"components/cart/cart-table-item":"<div\n  class=\"cart-item cart-table-item\"\n  data-id=\"{{id}}\"\n  data-qty=\"{{quantity}}\"\n  data-name=\"{{name}}\"\n>\n  <div class=\"image-column\">\n      {{#if type '===' 'GiftCertificate'}}\n        <div class=\"gift-card-image\">\n          <svg version=\"1.1\" id=\"Layer_1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0px\"\n             viewBox=\"0 0 100 81.9\" style=\"enable-background:new 0 0 100 81.9;\" xml:space=\"preserve\">\n            <path style=\"fill:currentColor\" d=\"M77.4,0.3L5.8,21.1c-4.3,1.2-6.7,5.7-5.5,10l12.3,42.3c1.2,4.3,5.7,6.7,10,5.5l71.6-20.8\n              c4.3-1.2,6.7-5.7,5.5-10L87.4,5.8C86.2,1.5,81.7-0.9,77.4,0.3z M84.1,6.8l2.6,8.8L48.1,26.8c-0.7-2.2-1.8-5.4-3.2-7.6\n              c0,0-7.6,1.7-11,5.6l-2.2-7.6L78.4,3.6C80.8,2.9,83.4,4.3,84.1,6.8z M6.8,24.5l17.4-5.1l2.4,8.1c-5.1-0.3-11.4,3.9-11.4,3.9\n              c0.4,1.5,1,3,1.6,4.5L6.2,39l-2.6-8.8C2.9,27.7,4.3,25.2,6.8,24.5z M15.9,72.5l-7.6-26l17.1-5c-0.8,2.5-2,5.5-3.6,9.1l5.2,9.4\n              c0,0,2.5-7.1,4.7-14.7l7.3,25.3l-17.4,5.1C19.2,76.3,16.6,74.9,15.9,72.5z M93.3,54.8L46.5,68.4l-8.3-28.5C45.9,46,55,52.5,55,52.5\n              L53,42c-3.4-2.3-6.2-4.3-8.4-6l44.3-12.9l7.6,26C97.1,51.5,95.7,54.1,93.3,54.8z\"/>\n            <ellipse style=\"fill:currentColor\" cx=\"53.6\" cy=\"79.3\" rx=\"41.3\" ry=\"2.6\"/>\n          </svg>\n        </div>\n      {{else}}\n        <a href=\"{{url}}\">\n          <img\n            src=\"{{getImage image 'product-grid-item' (cdn default_image)}}\"\n            alt=\"{{name}}\"\n          >\n        </a>\n      {{/if}}\n    </a>\n  </div>\n  <div class=\"main-column\">\n    <div class=\"cart-item-meta\">\n      {{#if type '===' 'GiftCertificate'}}\n        <a href=\"{{edit_url}}\">\n          <h5 class=\"product-name\">{{name}}</h5>\n        </a>\n      {{else}}\n        <a href=\"{{url}}\">\n          <h5 class=\"product-name\">{{name}}</h5>\n        </a>\n      {{/if}}\n\n      {{#if options.length}}\n        {{> components/products/options-list options=options}}\n      {{/if}}\n\n      {{#if configurable_fields.length}}\n        {{> components/products/customizations-list customizations=configurable_fields}}\n      {{/if}}\n\n      {{#if event_date}}\n        <div class=\"cart-item-detail product-event-summary\">\n          {{event_date.name}}:&nbsp;{{event_date.date}}\n        </div>\n      {{/if}}\n\n      {{#or options configurable_fields event_date}}\n        <a href=\"#\"\n          class=\"cart-item-edit-button link\"\n          data-item-edit=\"{{id}}\"\n        >\n          {{lang 'cart.change_options'}}\n        </a>\n      {{/or}}\n\n      {{#if show_gift_wrapping}}\n        {{> components/cart/cart-table-item-gift-wrap}}\n      {{/if}}\n\n      <div class=\"actions\">\n        {{#if type '===' 'GiftCertificate'}}\n          <a href=\"{{edit_url}}\">{{lang 'cart.product.edit'}}</a>\n          <span class=\"divider\">|</span>\n        {{/if}}\n        <a href=\"{{remove_url}}\" data-item-id=\"{{id}}\" class=\"remove-item\">{{lang 'cart.product.remove'}}</a>\n      </div>\n    </div>\n    <div class=\"cart-item-details-container\">\n      <div class=\"cart-item-details-header\">\n        <div class=\"cell cart-header-each\">Each</div>\n        <div class=\"cell cart-header-quantity\">Quantity</div>\n        <div class=\"cell cart-header-total\">Total</div>\n      </div>\n      <div class=\"cart-item-details\">\n        <div class=\"cell cart-content-each\">\n          <div class=\"price {{#if price_discounted}}price-discounted{{/if}}\">\n            {{price.formatted}}\n          </div>\n          {{#if price_discounted}}\n            <div class=\"price\">{{price_discounted.formatted}}</div>\n          {{/if}}\n        </div>\n        <div class=\"cell cart-content-quantity\">\n          <input type=\"number\" class=\"form-input quantity-input\" value=\"{{quantity}}\" min=\"1\" pattern=\"[0-9]*\">\n          <a class=\"update-quantity\" href=\"#\">{{lang 'cart.update'}}</a>\n        </div>\n        <div class=\"cell cart-content-total\">\n          <div class=\"price {{#if total_discounted}}price-discounted{{/if}}\">\n            {{total.formatted}}\n          </div>\n          {{#if total_discounted}}\n            <div class=\"price\">{{total_discounted.formatted}}</div>\n          {{/if}}\n        </div>\n      </div>\n    </div>\n  </div>\n</div>\n","components/cart/cart-table-item-gift-wrap":"<div class=\"item-gift-wrap\">\n  {{#if gift_wrapping}}\n    <div class=\"item-gift-wrap-info\">\n      {{lang 'cart.gift_wrapping.title'}} {{gift_wrapping.name}}\n      <span class=\"item-gift-wrap-change\">\n        (<button class=\"button-link item-gift-wrap-trigger\" data-item-gift-wrap=\"{{id}}\">{{lang 'cart.gift_wrapping.change'}}</button> or <a class=\"item-gift-wrap-remove\" href=\"{{gift_wrapping.remove_url}}\">{{lang 'cart.gift_wrapping.remove'}}</a>)\n      </span>\n    </div>\n    {{#if gift_wrapping.message}}\n      <div class=\"item-gift-wrap-info\">\n        {{lang 'cart.gift_wrapping.gift_message'}}\n        {{gift_wrapping.message}}\n      </div>\n    {{/if}}\n  {{else}}\n    {{lang 'cart.gift_wrapping.title'}} <button class=\"button-link item-gift-wrap-trigger item-gift-wrap-add\" data-item-gift-wrap=\"{{id}}\">{{lang 'cart.gift_wrapping.add'}}</button>\n  {{/if}}\n\n  <div class=\"gift-wrap-form-wrapper\" data-item-id=\"{{id}}\"></div>\n</div>\n","components/products/customizations-list":"<div class=\"cart-item-detail product-customizations-summary\">\n  {{#if customizations.length \">\" 3}}\n    {{#each customizations}}\n      <div class=\"cart-item-detail-item\">\n        {{name}}:&nbsp;{{{value}}}\n      </div>\n    {{/each}}\n  {{else}}\n    {{#each customizations}}\n      {{name}}:&nbsp;{{value}}{{#unless @last}}, {{/unless}}\n    {{/each}}\n  {{/if}}\n</div>\n","components/products/options-list":"<div class=\"cart-item-detail product-options-summary\">\n  {{#if options.length \">\" 3}}\n    {{#each options}}\n      <div class=\"cart-item-detail-item\">\n        {{name}}:&nbsp;{{{value}}}\n      </div>\n    {{/each}}\n  {{else}}\n    {{#each options}}\n      {{name}}:&nbsp;{{value}}{{#unless @last}}, {{/unless}}\n    {{/each}}\n  {{/if}}\n</div>\n"}
