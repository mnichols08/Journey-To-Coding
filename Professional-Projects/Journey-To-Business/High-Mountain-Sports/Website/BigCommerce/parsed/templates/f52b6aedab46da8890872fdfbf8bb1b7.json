{"components/brand/sidebar":"<h1 class=\"collection-name\">{{brand.name}}</h1>\n\n{{#if brand.faceted_search_enabled}}\n  {{#if brand.selected.items.length}}\n    {{> components/faceted-search/facet-selected brand.selected}}\n  {{/if }}\n\n  {{> components/faceted-search/facet-inputs facets=brand.facets}}\n{{/if }}\n\n<div class=\"collection-progress-overlay\"></div>\n","components/faceted-search/facet-inputs":"<a\n  class=\"button button-secondary button-small button-toggle-facets\"\n  data-toggled-text=\"{{lang 'category.sidebar.hide_filters'}}\"\n  data-toggle-faceted-search\n>\n  {{lang 'category.sidebar.show_filters'}}\n</a>\n\n<div class=\"faceted-search\">\n  {{#each facets}}\n    {{#if visible}}\n      {{#if items}}\n        <div\n          class=\"\n            faceted-search-filter\n            filter-type-{{type}}\n            {{#if selected }}is-open{{/if}}\n            {{#unless start_collapsed}}is-open{{/unless}}\n          \"\n        >\n          <button class=\"facet-section-header\" data-toggle-facet>\n            <h5 class=\"facet-section-title\">{{title}}</h5>\n            <div class=\"facet-section-toggle-icon\"></div>\n          </button>\n          {{{dynamicComponent 'components/faceted-search/facets'}}}\n        </div>\n      {{/if}}\n\n      {{#compare facet 'price'}}\n        <div class=\"faceted-search-filter filter-type-{{type}}\">\n          <button class=\"facet-section-header\" data-toggle-facet>\n            <h5 class=\"facet-section-title\">{{title}}</h5>\n            <div class=\"facet-section-toggle-icon\"></div>\n          </button>\n          {{{dynamicComponent 'components/faceted-search/facets'}}}\n        </div>\n      {{/compare}}\n    {{/if}}\n  {{/each}}\n</div>\n","components/faceted-search/facet-selected":"<div class=\"facet-selected-filters\">\n  <ul class=\"selected-filter-items\">\n    {{#each items}}\n      <li class=\"selected-filter-item\">\n        <a href=\"{{url}}\" title=\"{{title}}\" class=\"selected-filter-item-link\" data-faceted-search-facet>\n          {{#if param_name '===' 'rating'}}\n            {{lang 'search.faceted.rating.and_up' rating=title}}\n          {{else}}\n            {{title}}\n          {{/if}}\n          <span class=\"remove\">&times;</span>\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n\n  <a href=\"{{remove_all_url}}\" class=\"clear-all\" data-faceted-search-facet>{{lang 'search.faceted.selected.clear_all'}}</a>\n</div>\n","components/faceted-search/facets/hierarchy":"<div\n  class=\"facet-filter-wrapper {{#unless start_collapsed}}is-open{{/unless}}\"\n  data-facet-filter-wrapper\n>\n  <ul\n    class=\"faceted-search-filter-list facet-default\"\n    id=\"faceted-search-filter-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n    data-facet=\"{{facet}}\"\n    data-count=\"{{items_count}}\"\n  >\n    {{#each items}}\n      <li\n        class=\"\n          faceted-search-item\n          {{#compare @index 5 operator='>='}}hidden{{/compare}}\n        \"\n      >\n        <a\n          href=\"{{url}}\"\n          title=\"{{title}}\"\n          rel=\"nofollow\"\n          data-faceted-search-facet\n        >\n          <input\n            type=\"checkbox\"\n            class=\"facet-checkbox\"\n            {{#if selected}}checked{{/if}}\n          >\n          <span class=\"faceted-search-item-label\">\n            {{title}}\n            {{#if ../show_product_counts}}\n              <span class=\"faceted-search-item-count\">({{ count }})</span>\n            {{/if}}\n          </span>\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n\n  {{#if show_more_toggle}}\n    <a\n      href=\"#faceted-search-filter-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n      role=\"button\"\n      class=\"\n        toggleLink\n        faceted-show-more-button\n        {{#unless start_collapsed}}is-open{{/unless}}\n      \"\n      data-show-more-facets\n    >\n\n      <span class=\"toggle-link-text toggle-link-text-off\">\n        {{lang 'faceted_search.show_more'}}\n      </span>\n      <span class=\"toggle-link-text toggle-link-text-on\">\n        {{lang 'faceted_search.show_less'}}\n      </span>\n    </a>\n  {{/if}}\n</div>\n","components/faceted-search/facets/multi":"<div\n  class=\"facet-filter-wrapper {{#unless start_collapsed}}is-open{{/unless}}\"\n  data-facet-filter-wrapper\n>\n  <ul\n    class=\"faceted-search-filter-list facet-default\"\n    id=\"faceted-search-filter-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n    data-facet=\"{{facet}}\"\n    data-count=\"{{items_count}}\"\n  >\n    {{#each items}}\n      <li\n        class=\"\n          faceted-search-item\n          {{#compare @index 5 operator='>='}}hidden{{/compare}}\n        \"\n      >\n        <a\n          href=\"{{url}}\"\n          title=\"{{title}}\"\n          rel=\"nofollow\"\n          data-faceted-search-facet\n        >\n          <input\n            type=\"checkbox\"\n            class=\"facet-checkbox\"\n            {{#if selected}}checked{{/if}}\n          >\n          <span class=\"faceted-search-item-label\">\n            {{title}}\n            {{#if ../show_product_counts}}\n              <span class=\"faceted-search-item-count\">({{ count }})</span>\n            {{/if}}\n          </span>\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n\n  {{#if show_more_toggle}}\n    <a\n      href=\"#faceted-search-filter-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n      role=\"button\"\n      class=\"\n        toggleLink\n        faceted-show-more-button\n        {{#unless start_collapsed}}is-open{{/unless}}\n      \"\n      data-show-more-facets\n    >\n\n      <span class=\"toggle-link-text toggle-link-text-off\">\n        {{lang 'faceted_search.show_more'}}\n      </span>\n      <span class=\"toggle-link-text toggle-link-text-on\">\n        {{lang 'faceted_search.show_less'}}\n      </span>\n    </a>\n  {{/if}}\n</div>\n","components/faceted-search/facets/range":"<form class=\"faceted-search-filter-list\" method=\"get\" data-faceted-search-range>\n  {{#each current_selected_items}}\n    <input type=\"hidden\" name=\"{{param_name}}[]\" value=\"{{param_value}}\"/>\n  {{/each}}\n\n  <input type=\"hidden\" name=\"search_query\" value=\"{{search_query}}\">\n\n  {{#if sort}}\n    <input type=\"hidden\" name=\"sort\" value=\"{{sort}}\">\n  {{/if}}\n\n  <div class=\"facet-range-input-group\">\n    <div class=\"facet-range-inputs\">\n      <div class=\"form-field faceted-search-range-min\">\n        <input\n          class=\"form-input\"\n          name=\"min_price\"\n          min=\"0\"\n          type=\"number\"\n          value=\"{{min_price}}\"\n          placeholder=\"{{ lang 'category.sidebar.min_price'}}\"\n          required\n        />\n      </div>\n\n      <div class=\"form-field faceted-search-range-max\">\n        <input\n          class=\"form-input\"\n          name=\"max_price\"\n          min=\"0\"\n          type=\"number\"\n          value=\"{{max_price}}\"\n          placeholder=\"{{lang 'category.sidebar.max_price'}}\"\n          required\n        />\n      </div>\n    </div>\n\n    <input\n      class=\"button button-primary button-small facet-submit\"\n      type=\"submit\"\n      value=\"{{lang 'category.sidebar.update'}}\"\n    >\n  </div>\n</form>\n","components/faceted-search/facets/rating":"<ul class=\"faceted-search-filter-list\">\n  {{#each items}}\n    {{!--\n      TODO: Remove this check when platform bug fixed.\n      Rating facets sometimes provides an item without a URL.\n    --}}\n    {{#if url}}\n      <li class=\"faceted-search-item\">\n        <a\n          href=\"{{url}}\"\n          rel=\"nofollow\"\n          title=\"{{lang 'search.faceted.rating.and_up' rating=param_value}}\"\n          data-faceted-search-facet\n        >\n          <input\n            type=\"checkbox\"\n            class=\"facet-checkbox\"\n            {{#if selected}}checked{{/if}}\n          >\n\n          <span class=\"faceted-search-item-label facet-rating\">\n            <div class=\"rating-stars stars-{{size}}\" aria-hidden=\"true\">\n              {{#for 1 5}}\n                {{#if ../param_value '>=' $index}}\n                  <span class=\"star pxu-star checked\"></span>\n                {{else}}\n                  <span class=\"star pxu-star\"></span>\n                {{/if}}\n              {{/for}}\n            </div>\n\n            {{#if ../show_product_counts}}\n              <span class=\"faceted-search-item-count\">({{ count }})</span>\n            {{/if}}\n          </span>\n        </a>\n      </li>\n    {{/if}}\n  {{/each}}\n</ul>\n"}
