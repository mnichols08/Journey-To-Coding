{"components/quick-shop/quick-shop-images":"<div class=\"quick-shop-images-container\">\n  <div class=\"quick-shop-images\">\n    {{#each images}}\n      <img src=\"{{getImage this 'product'}}\" id=\"quick-shop-img-{{@index}}\" {{#if @first}}class=\"active\"{{/if}} alt=\"{{this.alt}}\">\n    {{/each}}\n  </div>\n  {{#compare images.length 1 operator=\">\"}}\n    <ul class=\"quick-shop-images-pagination\">\n      {{#each images}}\n        <li class=\"quick-shop-image-thumb {{#if @first}}active{{/if}}\">\n          <a href=\"#quick-shop-img-{{@index}}\">\n            <img src=\"{{getImage this 'thumbnail'}}\" alt=\"{{this.alt}}\">\n          </a>\n        </li>\n      {{/each}}\n    </ul>\n  {{/compare}}\n</div>\n"}
