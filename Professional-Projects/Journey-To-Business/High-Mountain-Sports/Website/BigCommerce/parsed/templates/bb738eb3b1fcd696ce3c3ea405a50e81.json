{"components/cart/shipping-calculator":"<div class=\"shipping-calculator\" id=\"shipping-calculator-wrapper\" data-shipping-calculator>\n  <form class=\"form calculator-form\" id=\"shipping-calculator-form\">\n    <div class=\"form-field form-field-small calculator-field-country\">\n      <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_calculator.select_a_country'}}</label>\n      <select class=\"form-input form-select\" id=\"shipping-country\" name=\"shipping-country\">\n        <option>{{lang 'cart.shipping_calculator.select_a_country'}}</option>\n        {{#each countries}}\n          <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n            {{name}}\n          </option>\n        {{/each}}\n      </select>\n    </div>\n\n    <div class=\"form-field form-field-small calculator-field-state\">\n      <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_calculator.select_a_state'}}</label>\n      {{#if states}}\n        <select class=\"form-input form-select\" id=\"shipping-state\" name=\"shipping-state\">\n          <option>{{lang 'cart.shipping_calculator.select_a_state'}}</option>\n          {{#each states}}\n            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n              {{name}}\n            </option>\n          {{/each}}\n        </select>\n      {{else}}\n        <input type=\"text\" class=\"form-input\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\n      {{/if}}\n    </div>\n\n    <div class=\"form-field form-field-small calculator-field-city\">\n      <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_calculator.select_a_city'}}</label>\n      <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_calculator.select_a_city'}}\">\n    </div>\n\n    <div class=\"form-field form-field-small calculator-field-zip\">\n      <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_calculator.zip_postal_code'}}</label>\n      <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_calculator.zip_postal_code'}}\">\n    </div>\n\n    <div class=\"form-field form-action\">\n      <button class=\"button button-small button-primary button-progress button-shipping-estimate-submit\" type=\"submit\" data-progress-text=\"{{lang 'cart.shipping_calculator.fetching_rates'}}\">\n        {{> components/common/spinner}}\n        <span class=\"button-text\">{{lang 'cart.shipping_calculator.estimate_shipping'}}</span>\n      </button>\n    </div>\n  </form>\n  <div class=\"shipping-quotes\" data-shipping-quotes></div>\n</div>\n","components/common/spinner":"<div class=\"pxu-fading-circle {{spinner_class}}\">\n  <div class=\"pxu-circle1 pxu-circle\"></div>\n  <div class=\"pxu-circle2 pxu-circle\"></div>\n  <div class=\"pxu-circle3 pxu-circle\"></div>\n  <div class=\"pxu-circle4 pxu-circle\"></div>\n  <div class=\"pxu-circle5 pxu-circle\"></div>\n  <div class=\"pxu-circle6 pxu-circle\"></div>\n  <div class=\"pxu-circle7 pxu-circle\"></div>\n  <div class=\"pxu-circle8 pxu-circle\"></div>\n  <div class=\"pxu-circle9 pxu-circle\"></div>\n  <div class=\"pxu-circle10 pxu-circle\"></div>\n  <div class=\"pxu-circle11 pxu-circle\"></div>\n  <div class=\"pxu-circle12 pxu-circle\"></div>\n</div>\n"}
