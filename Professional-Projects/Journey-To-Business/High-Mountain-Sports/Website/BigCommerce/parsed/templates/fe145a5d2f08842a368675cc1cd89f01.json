{"components/products/ratings":"<div class=\"rating-stars stars-{{size}}\" aria-hidden=\"true\">\n  {{#for 1 5}}\n      <span\n        class=\"star pxu-star {{#if ../this.rating '>=' $index}}star-full{{/if}}\"\n      >\n        <svg class=\"icon-star\">\n          <use xlink:href=\"#icon-star\"></use>\n        </svg>\n      </span>\n  {{/for}}\n</div>\n","components/products/review-item":"<article class=\"review-item review-item-{{class}}\">\n  <header class=\"review-item-header\">\n    {{#if show_rating}}\n      <div class=\"review-item-stars\">\n        {{> components/products/ratings rating=rating size=\"medium\"}}\n        <div class=\"review-item-score\"><span>{{rating}}</span>/5</div>\n      </div>\n    {{/if}}\n    <h5 class=\"review-item-title\">{{title}}</h5>\n    <div class=\"review-meta\">\n      {{lang 'product.reviews.written_by_on' name=name date=date}}\n    </div>\n  </header>\n  <p class=\"review-item-body\">{{text}}</p>\n</article>\n"}
